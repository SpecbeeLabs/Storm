        "drupal/password_policy": "^3.1",
        "drupal/username_enumeration_prevention": "^1.2",
        "drupal/flood_control": "^2.2",
        "drupal/seckit": "^2.0",
        "drupal/recaptcha": "^3.0"
    },
    "autoload": {
        "psr-4": {
